--game stuff
function reset_game()
	game_over = false
	score = 0

	level = 1
	levels = {
		100,
		200,
		300,
		400
	}

	make_player(64,-38)
	make_cam()
	make_env()

end

function check_score_to_change_level()
	if #levels > 0 and score > levels[1] then
		 level += 1
		 env = env_library[level]
			make_dust()
		 del(levels,levels[1])
	end
end

