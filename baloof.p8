function draw_gradient()
	for i=0,127 do
		local stripe = 6

		if i > 117 then
			stripe = 15
		elseif i > 114 or i == 98 then
			stripe = 9
		elseif i > 110 or i == 97 then
			stripe = 4
		elseif i > 105 or i == 95 then
			stripe = 2
		elseif i >= 99 or i == 96  then
			stripe = 13
		elseif i < 6 then
			stripe =1
		end

		line(0,i,127,i,stripe)
	end
end

